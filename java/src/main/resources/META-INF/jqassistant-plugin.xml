<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://schema.jqassistant.org/plugin/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant Dart Plugin" id="jqassistant.plugin.dart"
                    version="${project.version}">
    <description>${project.description}</description>
    <model>
        <class>org.jqassistant.plugin.dart.api.model.core.ClassDescriptor</class>
        <class>org.jqassistant.plugin.dart.api.model.core.FunctionDescriptor</class>
        <class>org.jqassistant.plugin.dart.api.model.core.DartDescriptor</class>
        <class>org.jqassistant.plugin.dart.api.model.core.DartScanDescriptor</class>
        <class>org.jqassistant.plugin.dart.api.model.core.EnumDescriptor</class>
        <class>org.jqassistant.plugin.dart.api.model.core.LibraryDescriptor</class>
        <class>org.jqassistant.plugin.dart.api.model.core.MixinDescriptor</class>
        <class>org.jqassistant.plugin.dart.api.model.core.PackageDescriptor</class>
        <class>org.jqassistant.plugin.dart.api.model.core.ParameterDescriptor</class>
        <class>org.jqassistant.plugin.dart.api.model.core.VariableDescriptor</class>

        <class>org.jqassistant.plugin.dart.api.model.core.LocalFileDescriptor</class>
    </model>
    <scope>
        <class>org.jqassistant.plugin.dart.api.DartScope</class>
    </scope>
    <scanner>
        <class>org.jqassistant.plugin.dart.impl.DartPackageScannerPlugin</class>
    </scanner>
</jqassistant-plugin>
